generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model SystemConfig {
  id             String     @id @default(cuid())
  key            String     @unique
  value          String     @db.LongText
  description    String?
  category       String
  type           ConfigType
  lastModifiedBy String?    @map("last_modified_by")
  createdAt      DateTime   @default(now()) @map("created_at")
  updatedAt      DateTime   @updatedAt @map("updated_at")
  isEditable     Boolean    @default(true) @map("is_editable")
  isSystem       Boolean    @default(false) @map("is_system")

  @@index([category])
  @@index([type])
  @@map("system_configs")
}

model SMSVerification {
  id        String   @id @default(cuid())
  phone     String
  code      String
  type      SMSType
  attempts  Int      @default(0)
  isUsed    Boolean  @default(false)
  expiresAt DateTime
  createdAt DateTime @default(now())

  @@map("sms_verifications")
}

model User {
  id                         String                  @id @default(cuid())
  openid                     String                  @unique
  nickname                   String?
  avatarUrl                  String?                 @map("avatar_url")
  phone                      String?                 @unique
  userNumber                 String?                 @unique @map("user_number")
  level                      UserLevel               @default(NORMAL)
  status                     UserStatus              @default(ACTIVE)
  parentId                   String?                 @map("parent_id")
  teamPath                   String?                 @map("team_path")
  teamLevel                  Int                     @default(1) @map("team_level")
  totalSales                 Float                   @default(0) @map("total_sales")
  totalBottles               Int                     @default(0) @map("total_bottles")
  directSales                Float                   @default(0) @map("direct_sales")
  teamSales                  Float                   @default(0) @map("team_sales")
  directCount                Int                     @default(0) @map("direct_count")
  teamCount                  Int                     @default(0) @map("team_count")
  cloudShopLevel             Int?                    @map("cloud_shop_level")
  hasWutongShop              Boolean                 @default(false) @map("has_wutong_shop")
  pointsBalance              Float                   @default(0) @map("points_balance")
  pointsFrozen               Float                   @default(0) @map("points_frozen")
  createdAt                  DateTime                @default(now()) @map("created_at")
  updatedAt                  DateTime                @updatedAt @map("updated_at")
  lastLoginAt                DateTime?               @map("last_login_at")
  referralCode               String?                 @unique @map("referral_code")
  teamCommissionCalculations CommissionCalculation[] @relation("UserCommissionCalculations")
  giftRecords                GiftRecord[]            @relation("UserGiftRecords")
  resolvedAlerts             InventoryAlert[]        @relation("InventoryAlertResolvers")
  inventoryAlerts            InventoryAlert[]        @relation("UserInventoryAlerts")
  inventoryItems             InventoryItem[]         @relation("UserInventoryItems")
  operatedLogs               InventoryLog[]          @relation("InventoryLogOperators")
  inventoryLogs              InventoryLog[]          @relation("UserInventoryLogs")
  inventoryStocks            InventoryStock[]        @relation("UserInventoryStocks")
  levelUpgrades              LevelUpgradeRecord[]    @relation("UserLevelUpgrades")
  shipments                  LogisticsShipment[]     @relation("UserShipments")
  notificationPreference     NotificationPreference? @relation("NotificationPreferences")
  notifications              Notification[]          @relation("UserNotifications")
  buyerOrders                Order[]                 @relation("BuyerOrders")
  sellerOrders               Order[]                 @relation("SellerOrders")
  paymentLogs                PaymentLog[]            @relation("PaymentLogOperators")
  payments                   PaymentRecord[]         @relation("UserPayments")
  refundApplications         PaymentRefund[]         @relation("UserRefundApplications")
  refundApprovals            PaymentRefund[]         @relation("UserRefundApprovals")
  teamPerformanceMetrics     PerformanceMetrics[]    @relation("UserPerformanceMetrics")
  pointsFrom                 PointsTransaction[]     @relation("PointsFrom")
  pointsTo                   PointsTransaction[]     @relation("PointsTo")
  purchaseOrders             PurchaseOrder[]         @relation("UserPurchaseOrders")
  salesOrders                PurchaseOrder[]         @relation("UserSalesOrders")
  shops                      Shop[]                  @relation("UserShops")
  teamMember                 TeamMember?             @relation("TeamMemberUser")
  ledTeams                   Team[]                  @relation("TeamLeader")
  parent                     User?                   @relation("UserParent", fields: [parentId], references: [id])
  children                   User[]                  @relation("UserParent")

  @@index([parentId], map: "users_parent_id_fkey")
  @@map("users")
}

model Shop {
  id              String           @id @default(cuid())
  userId          String           @map("user_id")
  shopType        ShopType
  shopLevel       Int              @default(1) @map("shop_level")
  shopName        String?          @map("shop_name")
  shopDescription String?          @map("shop_description")
  contactName     String?          @map("contact_name")
  contactPhone    String?          @map("contact_phone")
  address         String?
  totalSales      Float            @default(0) @map("total_sales")
  totalOrders     Int              @default(0) @map("total_orders")
  totalRevenue    Float            @default(0) @map("total_revenue")
  status          ShopStatus       @default(PENDING)
  createdAt       DateTime         @default(now()) @map("created_at")
  updatedAt       DateTime         @updatedAt @map("updated_at")
  inventoryAlerts InventoryAlert[] @relation("ShopInventoryAlerts")
  inventoryItems  InventoryItem[]  @relation("ShopInventoryItems")
  inventoryLogs   InventoryLog[]   @relation("ShopInventoryLogs")
  inventoryStocks InventoryStock[] @relation("ShopInventoryStocks")
  products        Product[]        @relation("ShopProducts")
  user            User             @relation("UserShops", fields: [userId], references: [id], onDelete: Cascade)

  @@index([userId], map: "shops_user_id_fkey")
  @@map("shops")
}

model ProductTag {
  id              String           @id @default(cuid())
  name            String           @unique
  color           String?
  description     String?
  sort            Int              @default(0)
  createdAt       DateTime         @default(now()) @map("created_at")
  updatedAt       DateTime         @updatedAt @map("updated_at")
  productTagLinks ProductTagLink[] @relation("ProductTagLinks")

  @@map("product_tags")
}

model ProductTagLink {
  id        String     @id @default(cuid())
  productId String     @map("product_id")
  tagId     String     @map("tag_id")
  createdAt DateTime   @default(now()) @map("created_at")
  product   Product    @relation("ProductTagLinks", fields: [productId], references: [id], onDelete: Cascade)
  tag       ProductTag @relation("ProductTagLinks", fields: [tagId], references: [id], onDelete: Cascade)

  @@unique([productId, tagId])
  @@index([tagId], map: "product_tag_links_tag_id_fkey")
  @@map("product_tag_links")
}

model ProductCategory {
  id          String            @id @default(cuid())
  name        String
  parentId    String?           @map("parent_id")
  level       Int               @default(1)
  sort        Int               @default(0)
  icon        String?
  description String?
  isActive    Boolean           @default(true) @map("is_active")
  createdAt   DateTime          @default(now()) @map("created_at")
  updatedAt   DateTime          @updatedAt @map("updated_at")
  parent      ProductCategory?  @relation("CategoryParent", fields: [parentId], references: [id])
  children    ProductCategory[] @relation("CategoryParent")
  products    Product[]

  @@index([parentId], map: "product_categories_parent_id_fkey")
  @@map("product_categories")
}

model ProductSpec {
  id              String           @id @default(cuid())
  productId       String           @map("product_id")
  name            String
  sku             String           @unique
  price           Float
  stock           Int              @default(0)
  minStock        Int              @default(10)
  images          String?
  isActive        Boolean          @default(true) @map("is_active")
  sort            Int              @default(0)
  createdAt       DateTime         @default(now()) @map("created_at")
  updatedAt       DateTime         @updatedAt @map("updated_at")
  inventoryAlerts InventoryAlert[] @relation("SpecInventoryAlerts")
  inventoryItems  InventoryItem[]  @relation("SpecInventoryItems")
  inventoryLogs   InventoryLog[]   @relation("SpecInventoryLogs")
  inventoryStocks InventoryStock[] @relation("SpecInventoryStocks")
  orderItems      OrderItem[]      @relation("OrderSpecItems")
  pricings        ProductPricing[]
  product         Product          @relation(fields: [productId], references: [id], onDelete: Cascade)

  @@index([productId], map: "product_specs_product_id_fkey")
  @@map("product_specs")
}

model ProductPricing {
  id        String       @id @default(cuid())
  productId String       @map("product_id")
  specId    String?      @map("spec_id")
  userLevel UserLevel    @map("user_level")
  price     Float
  createdAt DateTime     @default(now()) @map("created_at")
  updatedAt DateTime     @updatedAt @map("updated_at")
  product   Product      @relation(fields: [productId], references: [id], onDelete: Cascade)
  spec      ProductSpec? @relation(fields: [specId], references: [id])

  @@unique([productId, specId, userLevel])
  @@index([specId], map: "product_pricings_spec_id_fkey")
  @@map("product_pricings")
}

model Product {
  id              String           @id @default(cuid())
  categoryId      String           @map("category_id")
  name            String
  description     String?
  code            String           @unique
  sku             String           @unique
  basePrice       Float            @map("base_price")
  totalStock      Int              @default(0) @map("total_stock")
  minStock        Int              @default(10) @map("min_stock")
  images          String
  videoUrl        String?          @map("video_url")
  details         Json?
  status          ProductStatus    @default(ACTIVE)
  isFeatured      Boolean          @default(false) @map("is_featured")
  sort            Int              @default(0)
  scheduleOnAt    DateTime?        @map("schedule_on_at")
  scheduleOffAt   DateTime?        @map("schedule_off_at")
  createdAt       DateTime         @default(now()) @map("created_at")
  updatedAt       DateTime         @updatedAt @map("updated_at")
  shopId          String?          @map("shop_id")
  giftRecords     GiftRecord[]     @relation("ProductGiftRecords")
  inventoryAlerts InventoryAlert[] @relation("ProductInventoryAlerts")
  inventoryItems  InventoryItem[]  @relation("ProductInventoryItems")
  inventoryLogs   InventoryLog[]   @relation("ProductInventoryLogs")
  inventoryStocks InventoryStock[] @relation("ProductInventoryStocks")
  orderItems      OrderItem[]      @relation("OrderProductItems")
  pricings        ProductPricing[]
  specs           ProductSpec[]
  tags            ProductTagLink[] @relation("ProductTagLinks")
  category        ProductCategory  @relation(fields: [categoryId], references: [id])
  shop            Shop?            @relation("ShopProducts", fields: [shopId], references: [id])
  purchaseOrders  PurchaseOrder[]

  @@index([categoryId], map: "products_category_id_fkey")
  @@index([shopId], map: "products_shop_id_fkey")
  @@map("products")
}

model PurchaseOrder {
  id                String         @id @default(cuid())
  orderNo           String         @unique @map("order_no")
  buyerId           String         @map("buyer_id")
  sellerId          String         @map("seller_id")
  productId         String         @map("product_id")
  quantity          Int
  unitPrice         Float          @map("unit_price")
  totalAmount       Float          @map("total_amount")
  purchasePath      String         @map("purchase_path")
  commissionDetails Json?          @map("commission_details")
  status            PurchaseStatus @default(PENDING)
  createdAt         DateTime       @default(now()) @map("created_at")
  updatedAt         DateTime       @updatedAt @map("updated_at")
  completedAt       DateTime?      @map("completed_at")
  buyer             User           @relation("UserPurchaseOrders", fields: [buyerId], references: [id])
  product           Product        @relation(fields: [productId], references: [id])
  seller            User           @relation("UserSalesOrders", fields: [sellerId], references: [id])

  @@index([buyerId], map: "purchase_orders_buyer_id_fkey")
  @@index([productId], map: "purchase_orders_product_id_fkey")
  @@index([sellerId], map: "purchase_orders_seller_id_fkey")
  @@map("purchase_orders")
}

model InventoryLog {
  id                String                 @id @default(cuid())
  userId            String                 @map("user_id")
  productId         String                 @map("product_id")
  specId            String?                @map("spec_id")
  shopId            String?                @map("shop_id")
  operationType     InventoryOperationType @map("operation_type")
  quantity          Int
  quantityBefore    Int                    @map("quantity_before")
  quantityAfter     Int                    @map("quantity_after")
  warehouseType     WarehouseType          @map("warehouse_type")
  relatedOrderId    String?                @map("related_order_id")
  relatedPurchaseId String?                @map("related_purchase_id")
  adjustmentReason  String?                @map("adjustment_reason")
  operatorId        String                 @map("operator_id")
  operatorType      OperatorType           @map("operator_type")
  remarks           String?
  createdAt         DateTime               @default(now()) @map("created_at")
  operator          User                   @relation("InventoryLogOperators", fields: [operatorId], references: [id])
  product           Product                @relation("ProductInventoryLogs", fields: [productId], references: [id])
  shop              Shop?                  @relation("ShopInventoryLogs", fields: [shopId], references: [id])
  spec              ProductSpec?           @relation("SpecInventoryLogs", fields: [specId], references: [id])
  user              User                   @relation("UserInventoryLogs", fields: [userId], references: [id])

  @@index([userId], map: "inventory_logs_user_id_fkey")
  @@index([operatorId], map: "inventory_logs_operator_id_fkey")
  @@index([productId], map: "inventory_logs_product_id_fkey")
  @@index([shopId], map: "inventory_logs_shop_id_fkey")
  @@index([specId], map: "inventory_logs_spec_id_fkey")
  @@index([createdAt], map: "inventory_logs_created_at_idx")
  @@index([userId, warehouseType], map: "inventory_logs_user_warehouse_idx")
  @@index([createdAt, userId], map: "inventory_logs_date_user_idx")
  @@map("inventory_logs")
}

model InventoryAlert {
  id               String             @id @default(cuid())
  userId           String             @map("user_id")
  productId        String             @map("product_id")
  specId           String?            @map("spec_id")
  shopId           String?            @map("shop_id")
  alertType        InventoryAlertType @map("alert_type")
  currentQuantity  Int                @map("current_quantity")
  threshold        Int
  warehouseType    WarehouseType      @map("warehouse_type")
  isRead           Boolean            @default(false) @map("is_read")
  isResolved       Boolean            @default(false) @map("is_resolved")
  resolvedAt       DateTime?          @map("resolved_at")
  resolvedBy       String?            @map("resolved_by")
  resolveNote      String?            @map("resolve_note")
  notificationSent Boolean            @default(false) @map("notification_sent")
  lastNotifiedAt   DateTime?          @map("last_notified_at")
  createdAt        DateTime           @default(now()) @map("created_at")
  updatedAt        DateTime           @updatedAt @map("updated_at")
  product          Product            @relation("ProductInventoryAlerts", fields: [productId], references: [id])
  resolver         User?              @relation("InventoryAlertResolvers", fields: [resolvedBy], references: [id])
  shop             Shop?              @relation("ShopInventoryAlerts", fields: [shopId], references: [id])
  spec             ProductSpec?       @relation("SpecInventoryAlerts", fields: [specId], references: [id])
  user             User               @relation("UserInventoryAlerts", fields: [userId], references: [id])

  @@index([productId], map: "inventory_alerts_product_id_fkey")
  @@index([resolvedBy], map: "inventory_alerts_resolved_by_fkey")
  @@index([shopId], map: "inventory_alerts_shop_id_fkey")
  @@index([specId], map: "inventory_alerts_spec_id_fkey")
  @@index([userId], map: "inventory_alerts_user_id_fkey")
  @@map("inventory_alerts")
}

model InventoryItem {
  id             String        @id @default(cuid())
  userId         String        @map("user_id")
  productId      String        @map("product_id")
  specId         String?       @map("spec_id")
  shopId         String?       @map("shop_id")
  warehouseType  WarehouseType @map("warehouse_type")
  quantity       Int           @default(0)
  frozenQuantity Int           @default(0) @map("frozen_quantity")
  minStock       Int           @default(10) @map("min_stock")
  lastInAt       DateTime?     @map("last_in_at")
  lastOutAt      DateTime?     @map("last_out_at")
  lastAlertAt    DateTime?     @map("last_alert_at")
  alertCount     Int           @default(0) @map("alert_count")
  createdAt      DateTime      @default(now()) @map("created_at")
  updatedAt      DateTime      @updatedAt @map("updated_at")
  product        Product       @relation("ProductInventoryItems", fields: [productId], references: [id])
  shop           Shop?         @relation("ShopInventoryItems", fields: [shopId], references: [id])
  spec           ProductSpec?  @relation("SpecInventoryItems", fields: [specId], references: [id])
  user           User          @relation("UserInventoryItems", fields: [userId], references: [id], onDelete: Cascade)

  @@unique([userId, productId, specId, warehouseType])
  @@index([productId], map: "inventory_items_product_id_fkey")
  @@index([shopId], map: "inventory_items_shop_id_fkey")
  @@index([specId], map: "inventory_items_spec_id_fkey")
  @@map("inventory_items")
}

model PointsTransaction {
  id             String            @id @default(cuid())
  transactionNo  String            @unique @map("transaction_no")
  fromUserId     String?           @map("from_user_id")
  toUserId       String            @map("to_user_id")
  amount         Float
  type           TransactionType
  relatedOrderId String?           @map("related_order_id")
  description    String?
  metadata       Json?
  status         TransactionStatus @default(PENDING)
  balanceBefore  Float             @map("balance_before")
  balanceAfter   Float             @map("balance_after")
  createdAt      DateTime          @default(now()) @map("created_at")
  completedAt    DateTime?         @map("completed_at")
  fromUser       User?             @relation("PointsFrom", fields: [fromUserId], references: [id])
  toUser         User              @relation("PointsTo", fields: [toUserId], references: [id])

  @@index([fromUserId], map: "points_transactions_from_user_id_fkey")
  @@index([toUserId], map: "points_transactions_to_user_id_fkey")
  @@map("points_transactions")
}

model Order {
  id              String              @id @default(cuid())
  orderNo         String              @unique @map("order_no")
  totalAmount     Float               @map("total_amount")
  discountAmount  Float               @default(0) @map("discount_amount")
  finalAmount     Float               @map("final_amount")
  status          OrderStatus         @default(PENDING)
  paymentStatus   PaymentStatus       @default(UNPAID)
  createdAt       DateTime            @default(now()) @map("created_at")
  updatedAt       DateTime            @updatedAt @map("updated_at")
  paidAt          DateTime?           @map("paid_at")
  shippedAt       DateTime?           @map("shipped_at")
  deliveredAt     DateTime?           @map("delivered_at")
  buyerId         String              @map("buyer_id")
  buyerNotes      String?             @map("buyer_notes")
  cancelledAt     DateTime?           @map("cancelled_at")
  cashAmount      Float?              @map("cash_amount")
  completedAt     DateTime?           @map("completed_at")
  metadata        String?             @map("metadata")
  paymentMethod   PaymentMethod?
  pointsAmount    Float?              @map("points_amount")
  promotionInfo   String?             @map("promotion_info")
  sellerId        String?             @map("seller_id")
  sellerNotes     String?             @map("seller_notes")
  shippingAddress String?             @map("shipping_address")
  shippingFee     Float               @default(0) @map("shipping_fee")
  type            OrderType
  giftRecords     GiftRecord[]        @relation("OrderGiftRecords")
  logistics       LogisticsInfo?
  shipments       LogisticsShipment[] @relation("OrderShipments")
  items           OrderItem[]
  buyer           User                @relation("BuyerOrders", fields: [buyerId], references: [id])
  seller          User?               @relation("SellerOrders", fields: [sellerId], references: [id])
  payments        PaymentRecord[]     @relation("OrderPayments")

  @@index([buyerId], map: "orders_buyer_id_fkey")
  @@index([sellerId], map: "orders_seller_id_fkey")
  @@index([status], map: "orders_status_idx")
  @@index([paymentStatus], map: "orders_payment_status_idx")
  @@index([createdAt], map: "orders_created_at_idx")
  @@index([buyerId, status], map: "orders_buyer_status_idx")
  @@map("orders")
}

model OrderItem {
  id             String       @id @default(cuid())
  orderId        String       @map("order_id")
  productId      String       @map("product_id")
  productName    String       @map("product_name")
  productSpec    String?      @map("product_spec")
  productImage   String?      @map("product_image")
  quantity       Int
  discountAmount Float        @default(0) @map("discount_amount")
  finalPrice     Float        @map("final_price")
  metadata       String?      @map("metadata")
  skuId          String?      @map("sku_id")
  totalPrice     Float        @map("total_price")
  unitPrice      Float        @map("unit_price")
  order          Order        @relation(fields: [orderId], references: [id], onDelete: Cascade)
  product        Product      @relation("OrderProductItems", fields: [productId], references: [id])
  spec           ProductSpec? @relation("OrderSpecItems", fields: [skuId], references: [id])

  @@index([orderId], map: "order_items_order_id_fkey")
  @@index([productId], map: "order_items_product_id_fkey")
  @@index([skuId], map: "order_items_sku_id_fkey")
  @@map("order_items")
}

model PaymentRecord {
  id                   String          @id @default(cuid())
  orderId              String?         @map("order_id")
  paymentMethod        PaymentMethod   @map("payment_method")
  amount               Float
  status               PaymentStatus
  paidAt               DateTime?       @map("paid_at")
  createdAt            DateTime        @default(now()) @map("created_at")
  updatedAt            DateTime        @updatedAt @map("updated_at")
  channelOrderId       String?         @map("channel_order_id")
  channelTransactionId String?         @map("channel_transaction_id")
  clientIp             String?         @map("client_ip")
  currency             String          @default("CNY")
  description          String?
  expiredAt            DateTime?       @map("expired_at")
  extra                String?
  notifiedAt           DateTime?       @map("notified_at")
  notifyData           String?         @map("notify_data")
  paymentChannel       PaymentChannel  @map("payment_channel")
  paymentNo            String          @unique @map("payment_no")
  prepayId             String?         @map("prepay_id")
  subject              String?
  userAgent            String?         @map("user_agent")
  userId               String          @map("user_id")
  logs                 PaymentLog[]    @relation("PaymentLogs")
  order                Order?          @relation("OrderPayments", fields: [orderId], references: [id])
  user                 User            @relation("UserPayments", fields: [userId], references: [id])
  refunds              PaymentRefund[] @relation("PaymentRefunds")

  @@index([userId])
  @@index([orderId])
  @@index([channelOrderId])
  @@index([status])
  @@index([paymentChannel])
  @@index([createdAt])
  @@map("payment_records")
}

model LogisticsInfo {
  id                    String          @id @default(cuid())
  orderId               String          @unique @map("order_id")
  expressCompany        String          @map("express_company")
  trackingNumber        String          @map("tracking_number")
  status                LogisticsStatus @default(NOT_SHIPPED)
  currentLocation       String?         @map("current_location")
  estimatedDeliveryDate DateTime?       @map("estimated_delivery_date")
  tracks                String?
  metadata              String?         @map("metadata")
  createdAt             DateTime        @default(now()) @map("created_at")
  updatedAt             DateTime        @updatedAt @map("updated_at")
  order                 Order           @relation(fields: [orderId], references: [id], onDelete: Cascade)

  @@map("logistics_info")
}

model LogisticsCompany {
  id              String              @id @default(cuid())
  code            String              @unique
  name            String
  displayName     String              @map("display_name")
  logo            String?
  website         String?
  phone           String?
  serviceScope    LogisticsScope      @map("service_scope")
  deliveryTypes   String              @map("delivery_types")
  normalDelivery  Int                 @default(48) @map("normal_delivery")
  expressDelivery Int                 @default(24) @map("express_delivery")
  apiProvider     String?             @map("api_provider")
  apiConfig       Json?               @map("api_config")
  isApiEnabled    Boolean             @default(false) @map("api_enabled")
  isActive        Boolean             @default(true) @map("is_active")
  sort            Int                 @default(0)
  createdAt       DateTime            @default(now()) @map("created_at")
  updatedAt       DateTime            @updatedAt @map("updated_at")
  shipments       LogisticsShipment[]

  @@map("logistics_companies")
}

model LogisticsShipment {
  id                 String           @id @default(cuid())
  shipmentNo         String           @unique @map("shipment_no")
  orderId            String           @map("order_id")
  userId             String           @map("user_id")
  companyId          String           @map("company_id")
  trackingNumber     String           @map("tracking_number")
  expressType        String?          @map("express_type")
  senderName         String           @map("sender_name")
  senderPhone        String           @map("sender_phone")
  senderAddress      String           @map("sender_address")
  senderProvince     String?          @map("sender_province")
  senderCity         String?          @map("sender_city")
  senderDistrict     String?          @map("sender_district")
  senderPostalCode   String?          @map("sender_postal_code")
  receiverName       String           @map("receiver_name")
  receiverPhone      String           @map("receiver_phone")
